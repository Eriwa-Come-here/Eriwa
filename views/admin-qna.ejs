<div class="main-div">
  <div class="main-sidemenu">
    <ul class="menu-ul">
      <li>
        <a href="/admin/member" class="sidemenu-style"> 회원관리</a>
      </li>
      <li>
        <a href="/admin/post" class="sidemenu-style"> 게시글관리</a>
      </li>
      <li>
        <a href="/admin/analysis" class="sidemenu-style"> 통계분석</a>
        <ul class="smallmenu-ul">
          <li>
            <a href="/admin/analysis" class="smallmenu"> - 일일 게시글 분석</a>
          </li>
          <li>
            <a href="/admin/analysis" class="smallmenu"> - 사용자 분석</a>
          </li>
        </ul>
      </li>
      <li>
        <a href="/admin/qna" class="sidemenu-style"> 문의내역</a>
      </li>
    </ul>
  </div>
  <div class="main-contents">
    <div class="subject">문의 내역</div>
    <div>
      <table class="qna-table-style">
        <thead>
          <tr>
            <th scope="cols">No.</th>
            <th scope="cols">회원아이디</th>
            <th scope="cols">문의내용</th>
            <th scope="cols">구분</th>
          </tr>
        </thead>
        <% qna.forEach(s=>{ %>
        <tbody>
          <tr>
            <td scope="row"><%= s.qna_id %></td>
            <td>
              <span><%= s.user_id %></span>
            </td>
            <td>
              <a href="/admin/qna/response"><%= s.question %></a>
            </td>
            <td>
              <p>답변대기</p>
            </td>
          </tr>
        </tbody>
        <% }); %>
      </table>
    </div>
  </div>
</div>
