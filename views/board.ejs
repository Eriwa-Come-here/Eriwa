<div id="container">
    <% post.forEach(p => { %>
        <div id="all_contents">
            <div class="board_action">
                <h2><%= p.address1 %></h2>
                <a href="/board/post-writing"><div id="writingPost">글쓰기</div></a>
            </div>
            <div class="contents">
                <div class="listview_real_content" id="bod_content_end1">
                    <a href="<%= `/${p.image}` %>">
                        <div class="contents_container">
                            <div class="contents_explain">
                                <h2><%= p.title %></h2>
                                <h4><%= p.address1 %></h4>
                                <h5><%= p.user_id %></h5>
                                <p><%= p.content %></p>
                            </div>
                            <div class="img_main"><img src="../public/images/comment_icon.png" alt="메인 사진"></div>
                                
                            <div class="contents_info">
                                <a href="<%= `/${p.image}` %>">
                                    <div class="btn_jjim">
                                        <img src="/public/images/jjim_icon.png" alt="찜">
                                        <p>2,100</p>
                                    </div>
                                </a>
                                <a href="<%= `/${p.image}` %>">
                                    <div class="btn_comment">
                                        <img src="/public/images/comment_icon.png" alt="댓글">
                                        <p>100</p>
                                    </div>
                                </a>
                                <a href="<%= `/${p.image}` %>">
                                    <div class="btn_grade">
                                        <img src="/public/images/grade_icon.png" alt="별점">
                                        <p><%= p.grade %></p>
                                    </div>
                                </a>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="listview_real_content">
                    <a href="<%= `/${p.image}` %>">
                        <div class="contents_container">
                            <div class="contents_explain">
                                <h2><%= p.title %></h2>
                                <h4><%= p.address1 %></h4>
                                <h5><%= p.user_id %></h5>
                                <p><%= p.content %></p>
                            </div>
                            <div class="img_main"><img src="../public/images/comment_icon.png" alt="메인 사진"></div>
                                
                            <div class="contents_info">
                                <a href="<%= `/${p.image}` %>">
                                    <div class="btn_jjim">
                                        <img src="/public/images/jjim_icon.png" alt="찜">
                                        <p>2,100</p>
                                    </div>
                                </a>
                                <a href="<%= `/${p.image}` %>">
                                    <div class="btn_comment">
                                        <img src="/public/images/comment_icon.png" alt="댓글">
                                        <p>100</p>
                                    </div>
                                </a>
                                <a href="<%= `/${p.image}` %>">
                                    <div class="btn_grade">
                                        <img src="/public/images/grade_icon.png" alt="별점">
                                        <p><%= p.grade %></p>
                                    </div>
                                </a>
                            </div>
                        </div>
                    </a>
                </div>
            </div>
        </div>
    <% }); %>
</div>